{
  "expo": {
    "name": "Milne",
    "description": "The Milne Juice App",
    "slug": "milne",
    "privacy": "public",
    "version": "5.5.0",
    "orientation": "portrait",
    "primaryColor": "#cccccc",
    "icon": "./assets/icons/square-logo-2019-large-no.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F0F0F0"
    },
    "packagerOpts": {
      "assetExts": [
        "ttf",
        "mp4"
      ]
    },
    "ios": {
      "bundleIdentifier": "com.mtbi.milne",
      "supportsTablet": true,
      "buildNumber": "40"
    },
    "android": {
      "package": "com.no.milneandroid",
      "permissions": [
        "android.permission.CALL_PHONE",
        "android.permission.SEND_SMS"
      ],
      "versionCode": 22
    },
    "extra": {
      "eas": {
        "projectId": "cd82c1a0-fbf4-11e7-b222-0d7cf3692690"
      }
    },
    "plugins": [
      "expo-font",
      "expo-asset"
    ]
  }
}
